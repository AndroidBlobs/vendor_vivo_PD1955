#!/system/bin/sh
if ! applypatch --check EMMC:/dev/block/bootdevice/by-name/survival:100663296:a4193d157c0d1b548610fce40a6a15aedfbcb021; then
  applypatch  \
          --patch /system/wwh \
          --source EMMC:/dev/block/bootdevice/by-name/boot:100663296:3bb54abbc4e3220d3387fb023706ac6839ad75f5 \
          --target EMMC:/dev/block/bootdevice/by-name/survival:100663296:a4193d157c0d1b548610fce40a6a15aedfbcb021 && \
      log -t recovery "Installing new ww image: succeeded" || \
      log -t recovery "Installing new ww image: failed"
else
  log -t recovery "ww image already installed"
fi
